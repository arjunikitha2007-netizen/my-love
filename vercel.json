{
  "version": 2,
  "rewrites": [
    { 
      "source": "/save-location", 
      "destination": "/server.js" 
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
